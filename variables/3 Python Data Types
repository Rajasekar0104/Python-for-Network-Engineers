# Python automatically detects the data type (dynamic typing)

router_name = "Branch-Router"     # str → Device name
router_uptime_days = 120          # int → Uptime in days
cpu_usage = 23.5                 # float → CPU utilization
is_snmp_enabled = True            # bool → Feature state
interface_list = ["Gi0/0", "Gi0/1", "Gi0/2"]  # list → Interfaces

# type() shows the data type of a variable
print(type(router_name))
print(type(router_uptime_days))
print(type(cpu_usage))
print(type(is_snmp_enabled))
print(type(interface_list))


#No need to declare data types explicitly
